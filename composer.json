{
	"name": "fakharkhan/laravel-bootstrap",
	"description": "Laravel application with twitter bootstrap ready.",
    "author": "Fakhar Khan, CEO SoftPyrmaid, Pakistan's No. 1 Laravel Framework Developer",
	"keywords": ["framework", "laravel","twitter","bootstrap"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
